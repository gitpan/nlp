$first = <STDIN>;
print $first;
print "\n";
print "## THIS PROGRAM WAS AUTOMATICALLY COMPRESSED!\n";
print "## DO NOT EDIT THIS FILE! CHANGE THE SOURCES!\n";
print "\n";

while(<STDIN>) {
	next if (m|^[ \t]*#.*$|);
	next if (m|^[ \t]*$|);
	next if (m|^.+[ \t]+#[^"']+$|); 
	print;
}

